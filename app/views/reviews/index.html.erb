<%= debug params %>
<% @items.each do |item| %>
  <% if item.type == "cloze" %>
    <%= raw item.cloze_child.cloze.html %>
    <%= item.cloze_child.choices %>
    <%= item.answer %>
  <% elsif item.type == "choice" %>
    <%= raw item.choice.text %>
    <%= item.choice.choices %>
    <%= item.answer %>
  <% end %>
<% end %>


